generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./db/diningfm.sqlite"
}

model Episode {
  id        Int     @id @default(autoincrement())
  guid      String     @unique
  title     String
  publishedAt   DateTime @map(name: "published_at")
  enclosureUrl String @map(name: "enclosure_url")
  enclosureLength Int @map(name: "enclosure_length")
  enclosureType String @map(name: "enclosure_type")
  description String
  explicit Boolean
  duration String
  imageUrl String @map(name: "image_url")
  season Int
  number Int
  episode_type String
  spotifyId String? @map(name: "spotify_id")
  applePodcastId String? @map(name: "apple_podcast_id")
  youtubeId String? @map(name: "youtube_id")
  summary String?
  @@map(name: "episodes")
}
